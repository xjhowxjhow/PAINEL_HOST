
<script> 
var sockets = io();
sockets.on('att-painel-guiche', function(data){
    console.log('Nova senha emitida atualizar pagina');
    location.reload(true);
}); 
</script>

<h1> Painel funcionario Gighe {{id}}</h1>
<div class="btn-fecha-guiche flex-coll" id="{{id}}" onclick="fechaGuiche(this.id)">
    <p>Fechar guiche</p>
</div>
    

<div class="main-adm flex-row">

    <div class ="menu-bar-adm flex-coll">

            <div class="menu-bar-adm-item2 flex-coll">
                <p>Configurações</p>
            </div>

            <div class="menu-bar-adm-item flex-coll" id="{{id}}" onclick="choice_guiche(this.id)">
                <a>Atendimento</a>
            </div>
            <div class="menu-bar-adm-item flex-coll active-adm" id="{{id}}" onclick="redirect_fila_espera(this.id)">
                <a>Fila de Espera</a>
            </div>


            <div class="menu-bar-adm-item flex-coll">
                <a href="/adm/relatorios">Configurações Painel</a>
            </div>
    </div>



    <div class="group-content-adm flex-coll">
        
        <div class ="header-adm flex-row"></div>
        <div class="content-adm flex-coll">
            {{!-- lsita giches  --}}

            <div class="modal-content" id="modal-giche">
                <div class="modal-header flex-row">
                    <h2>Novo Giche</h2>
                    <div class="btn-close-modal-add-guiche flexbotao" id="btn-close-modal-add-guiche">
                        <p class="p-inside-btn">FECHAR</p>
                    </div>
                </div>
                <div class="modal-body">
                    {{!-- info new giche --}}
                    <div class="box-inputs-new-giches flex-row">
                        <div class="box-inputs-new-giches-item flex-coll">
                            <label >Nome</label>
                            <input type="text" name="nome_guiche_new" id="nome-guiche-new">
                        </div>
                        <div class=" box-inputs-new-giches-item flex-coll">
                            <label>Descrição</label>
                            <input type="text" name="desc_guiche_new" id="desc-guiche-new">
                        </div>
                    </div>
                </div>
                <div class="modal-footer flex-row">
                     <div class="btn-add-guiche-cancelar flexbotao" id="btn-add-guiche-cancelar">
                        <p class="p-inside-btn">Cancelar</p>
                     </div>
                     <div class="btn-add-guiche-salvar flexbotao" id="">
                        <p>Salvar</p>
                     </div>
                </div>
            </div>
            
            <div class="box-title-giches flex-coll">
                <h1>Fila de Espera</h1>
            </div>

            <div class="list-giches flex-coll">
                <div class="header-titles-giches flex-row">
                    <div class="title-giches flex-coll">
                        <p>Tipo</p>
                    </div>
                    <div class="title-giches flex-coll">
                        <p>Senha</p>
                    </div>
                    <div class="title-giches flex-coll">
                        <p>Ativo</p>
                    </div>
                    <div class="title-giches flex-coll">
                        <p>Editar</p>
                    </div>
                    <div class="title-giches flex-row">
                        <p>Excluir</p>
                    </div>
                </div>
                {{#each posts}}
                <div class="row-list-giches flex-row">           
                        <div class="item-giches flex-coll">
                            <p>{{dataValues.tipo_senha}}</p>
                        </div>
                        <div class="item-giches flex-coll">
                            <p>{{dataValues.senha}}</p>
                        </div>
                        <div class="item-giches flex-coll">
                            <p>{{dataValues.status_senha}}</p>
                        </div>
                        <div class="item-giches flex-coll">
                            <div class="btn-edit-giches flex-coll">
                                <p>Editar</p>
                            </div>
                        </div>
                        <div class="item-giches flex-row">
                            <div class="btn-delet-giches flex-coll" id="{{dataValues.id}}" onclick="">
                                <p>Excluir</p>
                            </div>
                        </div>
                </div>
                {{/each}}



            </div>
        </div>
    

</div>